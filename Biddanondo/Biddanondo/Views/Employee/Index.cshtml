@model IEnumerable<Biddanondo.Models.ItemModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.RestaurantName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Location)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    foreach(var new_item in ViewBag.Product){
        if(item.EmployeeName==new_item.EmployeeName){
             <tr>

                 <td>
                  @item.RestaurantName
                 </td>
                 <td>
                 @item.EmployeeName
                 </td>
                 <td>
                 @item.Location
                 </td>
                 <td>
                 </td>
             </tr>
        }
                    
    }
    
}

</table>
